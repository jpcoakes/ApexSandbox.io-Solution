public Integer nextPrime(Integer num) {
    //code here
    Boolean foundPrime = false;
    Integer n = num;
    while(!foundPrime){
        n = n + 1;
        foundPrime = isPrime(n);
    }    
    return n;
}
public Boolean isPrime(Integer num) {
    for(Integer i = 2; i < num; i++){
        if(Math.mod(num,i) == 0){
            return false;
        }
    }
    return true;
}