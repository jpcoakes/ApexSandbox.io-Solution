public static Integer findLargest(List<Integer> nums) {
	//code here
    Integer length = nums.size();
    Integer maxIndex;
    if(nums == null){
        return null;
    }
    Integer i = 1;
    for(i : nums){
        if(nums[i-1] > nums[i]){
            maxIndex = i-1;
        }
    }
    return nums[maxIndex];
}