public static Integer findLargest(List<Integer> nums) {
	//code here
    Integer length = nums.size();
    Integer maxIndex;
    if(nums == null){
        return null;
    }
    
    for(Integer i : nums){
        if(nums[i] > nums[i+1]){
            maxIndex = i;
        }
    }
    return nums[maxIndex];
}